# Encryption key (32 bytes hex): openssl rand -hex 32
SECRET_KEY=

# PostgreSQL connection string
DATABASE_URL=postgresql://mtgimport:mtgimport@localhost:5435/mtg_import

# Installation domain restriction (regex pattern)
ALLOWED_DOMAIN_PATTERN=/.*/

# APL storage (default: file for local dev)
APL=file

# Log level
APP_LOG_LEVEL=info

# Override iframe/API URLs (for local dev or Docker)
APP_IFRAME_BASE_URL=http://localhost:3005
APP_API_BASE_URL=http://localhost:3005

# App identity
MANIFEST_APP_ID=saleor.app.mtg-import
APP_NAME=MTG Import
