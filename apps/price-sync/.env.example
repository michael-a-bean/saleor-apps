# Price Sync Worker Configuration
# Copy to .env and fill in values

# Database (shared with inventory-ops and buylist)
DATABASE_URL=postgresql://saleor:saleor@localhost:5432/saleor

# Saleor app installation ID (from AppInstallation table)
INSTALLATION_ID=your-installation-uuid-here

# Saleor channel for price associations
SALEOR_CHANNEL_ID=default-channel

# Scryfall API configuration
SCRYFALL_API_BASE_URL=https://api.scryfall.com
SCRYFALL_RATE_LIMIT_MS=100

# TCGPlayer API (optional, not yet implemented)
# TCGPLAYER_API_KEY=
# TCGPLAYER_API_SECRET=

# Price provider preference
PRICE_PROVIDER=scryfall

# Sync configuration
BATCH_SIZE=100
CONCURRENCY=1

# Logging
LOG_LEVEL=info
